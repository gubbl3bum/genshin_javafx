<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<?import java.net.URL?>
<VBox maxHeight="619.0" maxWidth="-900.0" minHeight="619.0" minWidth="900.0" prefHeight="600.0" prefWidth="900.0"
      xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.genshin_javafx.controllers.ShowBannersController">
    <AnchorPane prefHeight="663.0" prefWidth="900.0" style="-fx-background-color: RGB(32,36,66);">
        <Text fill="#d0d8f3" layoutX="366.0" layoutY="82.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Banners"
              styleClass="text-title-custom">
        </Text>
        <Button fx:id="Menu" layoutX="667.0" layoutY="529.0" mnemonicParsing="false" prefHeight="52.0"
                prefWidth="182.0" text="Menu" styleClass="button-custom">
        </Button>
        <TableView fx:id="tableView" layoutX="52.0" layoutY="111.0" prefHeight="373.0" prefWidth="797.0"
                   style="-fx-background-color: RGB(40,50,90);">
            <columns>
                <TableColumn fx:id="idBanner" prefWidth="36.0" text="Id"/>
                <TableColumn fx:id="name" prefWidth="112.0" text="Name"/>
                <TableColumn fx:id="dateStart" prefWidth="87.0" text="Date Start"/>
                <TableColumn fx:id="dateEnd" prefWidth="84.0" text="Date End"/>
                <TableColumn fx:id="character5" prefWidth="115.0" text="Character 5*"/>
                <TableColumn fx:id="character4_1" prefWidth="102.0" text="Character 4*"/>
                <TableColumn fx:id="character4_2" prefWidth="92.0" text="Character 4*"/>
                <TableColumn fx:id="character4_3" prefWidth="106.0" text="Character 4*"/>
                <TableColumn fx:id="version" prefWidth="62.0" text="Version"/>
            </columns>
        </TableView>
        <stylesheets>
            <URL value="@style.css"/>
        </stylesheets>
    </AnchorPane>
</VBox>
