<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import java.net.URL?>

<VBox maxHeight="619.0" maxWidth="-900.0" minHeight="619.0" minWidth="900.0" prefHeight="600.0" prefWidth="900.0"
      xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.genshin_javafx.controllers.SearchingCharacterController">
    <AnchorPane prefHeight="663.0" prefWidth="900.0" style="-fx-background-color: RGB(32,36,66);">
        <AnchorPane layoutX="66.0" layoutY="142.0" prefHeight="302.0" prefWidth="774.0" styleClass="anchor-custom">
            <JFXComboBox fx:id="age" layoutX="499.0" layoutY="110.0" prefHeight="25.0" prefWidth="205.0"/>
            <Text fill="#d0d8f3" layoutX="433.0" layoutY="129.0" strokeType="OUTSIDE" strokeWidth="0.0"
                  text="Age:" styleClass="text-criteria-custom">
            </Text>
            <JFXComboBox fx:id="weapon" layoutX="499.0" layoutY="169.0" prefHeight="25.0" prefWidth="205.0"/>
            <Text fill="#d0d8f3" layoutX="415.0" layoutY="189.0" strokeType="OUTSIDE" strokeWidth="0.0"
                  text="Weapon:" styleClass="text-criteria-custom">
            </Text>
            <JFXComboBox fx:id="gender" layoutX="499.0" layoutY="56.0" prefHeight="25.0" prefWidth="205.0"/>
            <Text fill="#d0d8f3" layoutX="433.0" layoutY="75.0" strokeType="OUTSIDE" strokeWidth="0.0"
                  text="Sex:" styleClass="text-criteria-custom">
            </Text>
            <JFXComboBox fx:id="region" layoutX="127.0" layoutY="169.0" prefHeight="25.0" prefWidth="205.0"/>
            <Text fill="#d0d8f3" layoutX="41.0" layoutY="188.0" strokeType="OUTSIDE" strokeWidth="0.0"
                  text="Region:" styleClass="text-criteria-custom">
            </Text>
            <JFXComboBox fx:id="element" layoutX="127.0" layoutY="110.0" prefHeight="25.0" prefWidth="205.0"/>
            <Text fill="#d0d8f3" layoutX="35.0" layoutY="132.0" strokeType="OUTSIDE" strokeWidth="0.0"
                  text="Element:" styleClass="text-criteria-custom">
            </Text>
            <JFXComboBox fx:id="name" layoutX="127.0" layoutY="56.0" prefHeight="25.0" prefWidth="205.0"/>
            <Text fill="#d0d8f3" layoutX="48.0" layoutY="73.0" strokeType="OUTSIDE" strokeWidth="0.0"
                  text="Name:" styleClass="text-criteria-custom">
            </Text>
            <JFXComboBox fx:id="quality" layoutX="127.0" layoutY="223.0" prefHeight="25.0" prefWidth="205.0"/>
            <Text fill="#d0d8f3" layoutX="40.0" layoutY="243.0" strokeType="OUTSIDE" strokeWidth="0.0"
                  text="Quality:" styleClass="text-criteria-custom">
            </Text>
        </AnchorPane>
        <Text fill="#d0d8f3" layoutX="259.0" layoutY="85.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Search Character" styleClass="text-title-custom">
        </Text>
        <Button fx:id="searching_character" layoutX="402.0" layoutY="475.0" mnemonicParsing="false"
                prefHeight="52.0" prefWidth="182.0" text="Search" styleClass="button-custom">
        </Button>
        <Button fx:id="menu" layoutX="658.0" layoutY="475.0" mnemonicParsing="false" prefHeight="52.0"
                prefWidth="182.0" text="Menu" styleClass="button-custom" >
        </Button>
        <stylesheets>
            <URL value="@style.css"/>
        </stylesheets>
    </AnchorPane>
</VBox>
