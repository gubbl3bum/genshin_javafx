<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import java.net.URL?>

<VBox maxHeight="619.0" maxWidth="-900.0" minHeight="619.0" minWidth="900.0" prefHeight="600.0" prefWidth="900.0"
      xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.genshin_javafx.controllers.SearchingBannerController">
    <AnchorPane prefHeight="663.0" prefWidth="900.0" style="-fx-background-color: RGB(32,36,66);">
        <AnchorPane layoutX="52.0" layoutY="146.0" prefHeight="324.0" prefWidth="796.0"
                    styleClass="anchor-custom">
            <JFXComboBox fx:id="version" layoutX="558.0" layoutY="196.0" prefHeight="25.0" prefWidth="205.0"/>
            <Text fill="#d0d8f3" layoutX="454.0" layoutY="216.0" strokeType="OUTSIDE" strokeWidth="0.0"
                  text="Version:" styleClass="text-criteria-custom">
            </Text>

            <JFXComboBox fx:id="character4_1" layoutX="558.0" layoutY="137.0" prefHeight="25.0"
                       prefWidth="205.0"/>
            <Text fill="#d0d8f3" layoutX="56.0" layoutY="267.0" strokeType="OUTSIDE" strokeWidth="0.0"
                  text="Date: " styleClass="text-criteria-custom">
            </Text>
            <DatePicker fx:id="dateBetween" layoutX="142.0" layoutY="247.0" prefHeight="25.0"
                        prefWidth="205.0"/>
        </AnchorPane>
        <Text fill="#d0d8f3" layoutX="291.0" layoutY="83.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Search Banner" styleClass="text-title-custom">
        </Text>

        <JFXComboBox fx:id="name" layoutX="195.0" layoutY="223.0" prefHeight="25.0" prefWidth="205.0"/>
        <Text fill="#d0d8f3" layoutX="107.0" layoutY="243.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name:"
              styleClass="text-criteria-custom">
        </Text>

        <DatePicker fx:id="dateStart" layoutX="196.0" layoutY="283.0" prefHeight="25.0" prefWidth="205.0"/>
        <Text fill="#d0d8f3" layoutX="84.0" layoutY="303.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Date start:" styleClass="text-criteria-custom">
        </Text>

        <DatePicker fx:id="dateEnd" layoutX="195.0" layoutY="339.0" prefHeight="25.0" prefWidth="205.0"/>
        <Text fill="#d0d8f3" layoutX="91.0" layoutY="359.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Date end:"
              styleClass="text-criteria-custom">
        </Text>

        <JFXComboBox fx:id="character5" layoutX="611.0" layoutY="223.0" prefHeight="25.0" prefWidth="205.0"/>
        <Text fill="#d0d8f3" layoutX="483.0" layoutY="243.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Character 5*:" styleClass="text-criteria-custom">
        </Text>

        <Text fill="#d0d8f3" layoutX="483.0" layoutY="303.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Character 4*:" styleClass="text-criteria-custom">
        </Text>

        <Button fx:id="Searching_banner_button" layoutX="404.0" layoutY="493.0" mnemonicParsing="false"
                prefHeight="52.0" prefWidth="182.0" text="Search" styleClass="button-custom">
        </Button>
        <Button fx:id="Menu_button" layoutX="654.0" layoutY="493.0" mnemonicParsing="false" prefHeight="52.0"
                prefWidth="182.0" text="Menu" styleClass="button-custom">
        </Button>
        <stylesheets>
            <URL value="@style.css"/>
        </stylesheets>
    </AnchorPane>
</VBox>
