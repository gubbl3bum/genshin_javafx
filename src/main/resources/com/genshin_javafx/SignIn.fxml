<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<?import java.net.URL?>
<VBox maxHeight="619.0" maxWidth="-900.0" minHeight="619.0" minWidth="900.0" prefHeight="600.0" prefWidth="900.0"
      xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.genshin_javafx.controllers.SignInController">
    <AnchorPane prefHeight="663.0" prefWidth="900.0" style="-fx-background-color: RGB(32,36,66);">
        <AnchorPane layoutX="228.0" layoutY="158.0" prefHeight="285.0" prefWidth="444.0"
                    styleClass="anchor-custom"/>
        <Text fill="#d0d8f3" layoutX="377.0" layoutY="92.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Sign in"
            styleClass="text-title-custom">
        </Text>

        <TextField fx:id="login" layoutX="403.0" layoutY="215.0" prefHeight="25.0" prefWidth="205.0"/>
        <Text fill="#d0d8f3" layoutX="315.0" layoutY="235.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Login:"
            styleClass="text-criteria-custom">
        </Text>

        <PasswordField fx:id="password" layoutX="403.0" layoutY="275.0" prefHeight="25.0" prefWidth="205.0"/>
        <Text fill="#d0d8f3" layoutX="292.0" layoutY="295.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Password:" styleClass="text-criteria-custom">
        </Text>

        <Button fx:id="Sign_in_button" layoutX="359.0" layoutY="368.0" mnemonicParsing="false" prefHeight="52.0"
                prefWidth="182.0" text="Sign in" styleClass="button-custom">
        </Button>
        <Button fx:id="Start_button" layoutX="654.0" layoutY="493.0" mnemonicParsing="false" prefHeight="52.0"
                prefWidth="182.0" text="Start" styleClass="button-custom">
        </Button>
        <stylesheets>
            <URL value="@style.css"/>
        </stylesheets>
    </AnchorPane>
</VBox>
